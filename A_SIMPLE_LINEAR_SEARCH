#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n=10;
	int a[n];
	for(int i=0;i<n;i++)
	{
	    cin>>a[i];
	}
	int k;
	cin>>k;
	for(int j=0;j<n;j++)
	{
	    if (a[j]==k)
	    {
	        cout<<"The key is at "<<j<<" th position in the array and the key is "<<a[j]<<"\n";
	    }
	}
	return 0;
}
